disguise = require('./LuaUTypes').disguise

function isEmpty(a: {[any]: any}): boolean return next(a) == nil end

function deepSoftIndex<A, B>(t: {[A]: any}, ...: A): B?
	for i = 1, select('#', ...) do
		local index = select(i, ...)
		t = t[index]

		if not t then return end
	end

